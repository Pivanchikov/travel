{
    "extends": ["tslint:latest", "tslint-react", "tslint-config-airbnb", "tslint-eslint-rules", "tslint-config-prettier"],
    "rules": {
      "no-submodule-imports": false,
      "no-shadowed-variable": false,
      "no-var-requires": false,
      "no-unused-expression": [true, "allow-fast-null-checks", "allow-tagged-template"],
      "variable-name": [true, "allow-pascal-case", "allow-leading-underscore"],
      "ordered-imports": [
        true,
        {
          "import-sources-order": "any",
          "named-imports-order": "case-insensitive"
        }
      ],
      "member-access": false,
      "member-ordering": false,
      "object-literal-sort-keys": false,
      "object-shorthand-properties-first": false,
      "no-object-literal-type-assertion": false,
      "interface-name": false,
      "jsx-boolean-value": false,
      "jsx-no-lambda": false,
      "array-type": [true, "array"],
      "import-name": [true, { "react": "React" }]
    }
  }
  